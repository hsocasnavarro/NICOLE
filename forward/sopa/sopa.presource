      LOGICAL , SAVE :: firsttime = .TRUE.
 
      COMMON /ATWT  / ATW , SUMA
!
      COMMON /MODEL / NE(MDEP) , TEMp(MDEP) , PE(MDEP) , NATom(MDEP)
